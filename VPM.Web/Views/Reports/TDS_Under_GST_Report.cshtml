
@{
    ViewBag.Title = "TDS Under GST";
}
<div class="container-fluid body-content" ng-controller="TDSUnderGSTReport">
    <div class="row">
        <div class="col-md-12 p-0">
            <ul class="nav">
                <li class="nav-item"> <a class="nav-link text-dark" href="javascript:void(0);"><strong>TDS Under GST</strong></a> </li>
                </ul>
            <hr class="m-0">
        </div>
    </div>

    <div class="row">
        <div class="col-md-12 p-0">
            <form>
                <div class="form-row mt-4">
                    <div class="form-group col-md-3">
                        <label for="">From Date</label>
                        <input type="date" class="form-control" id="fromDate" ng-model="fromDate">
                    </div>
                    <div class="form-group col-md-3">
                        <label for="">To Date</label>
                        <input type="date" class="form-control" id="toDate" ng-model="toDate">
                    </div>
                    <div class="form-group col-md-3 mt-4">
                        <button type="submit" class="btn btn-primary" ng-click="Search(fromDate,toDate)">
                            <span ng-show="searchButtonText == 'Searching'">
                                <img style="width:20px;height:20px;" src="~/images/loader.gif" />
                            </span>
                            {{ searchButtonText }}
                        </button>
                    </div>
                </div>
            </form>
